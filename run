#/bin/bash
cd RoboticSandbox
echo 1
git pull
echo 2
mv main.py ../
echo 3
mv object_tf_service.py ../
mv object_tf_client.py ../
echo 4
cd ../
echo 5
rm object_tf_service.pyc
rm object_tf_client.pyc
echo 6
python main.py
echo 7
mv main.py RoboticSandbox/
mv object_tf_client.py RoboticSandbox/
mv object_tf_service.py RoboticSandbox/
echo Done
